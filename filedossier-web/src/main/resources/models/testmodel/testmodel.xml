<?xml version="1.0" encoding="UTF-8"?>
<package
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xmlns="urn:ru:ilb:filedossier:ddl"
    xsi:schemaLocation="urn:ru:ilb:filedossier:ddl ../../../../../filedossier-ddl/src/main/resources/schemas/filedossier/ddl.xsd">
    <code>testmodel</code>
    <name>Тестовый пакет досье</name>
    <dossier>
        <code>TEST</code>
        <name>Тестовое досье</name>
        <dossierFile>
            <code>fairpricecalc</code>
            <name>fairpricecalc (.ods test)</name>
            <readonly>false</readonly>
            <required>true</required>
            <hidden>false</hidden>
            <variation>
                <mediaType>application/json</mediaType>
                <representation>
                    <mediaType>application/vnd.oasis.opendocument.spreadsheet</mediaType>
                    <stylesheet>representations/fairpriceorder/content.xsl</stylesheet>
                    <template>representations/fairpriceorder/template.ods</template>
            </representation>
            </variation>
        </dossierFile>
        <dossierFile>
            <code>pts</code>
            <name>Скан ПТС</name>
            <readonly>false</readonly>
            <required>true</required>
            <hidden>false</hidden>
            <variation>
                <mediaType>image/jpeg</mediaType>
            </variation>
            <variation>
                <mediaType>application/pdf</mediaType>
            </variation>
        </dossierFile>
        <dossierFile>
            <code>bailregistry</code>
            <name>reestr zalogov</name>
            <readonly>false</readonly>
            <required>true</required>
            <hidden>false</hidden>
            <variation>
                <mediaType>application/json</mediaType>
                <representation>
                    <mediaType>application/xml</mediaType>
                </representation>
            </variation>
        </dossierFile>
    </dossier>
</package>
